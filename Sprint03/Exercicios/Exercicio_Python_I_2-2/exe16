def soma_numeros(string_numeros):
    # Divide a string em uma lista de números usando a vírgula como separador
    numeros = string_numeros.split(',')
    
    # Converte cada número de string para inteiro e calcula a soma
    soma = sum(int(numero) for numero in numeros)
    
    return soma

# String de entrada
string_numeros = "1,3,4,6,10,76"

# Chamando a função e imprimindo a soma com a formatação correta
resultado = soma_numeros(string_numeros)
print(f"{resultado}")  # Formatação da saída
