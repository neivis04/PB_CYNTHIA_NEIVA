class Calculo:
    def somar(self, x, y):
        # Método para somar X e Y
        return x + y  

    def subtrair(self, x, y):
        # Método para subtrair Y de X
        return x - y  

# Valores de teste
x = 4
y = 5

# Criando uma instância da classe Calculo
calculo = Calculo()

# Realizando as operações e imprimindo os resultados
resultado_soma = calculo.somar(x, y)
resultado_subtracao = calculo.subtrair(x, y)

print(f"Somando: {x} + {y} = {resultado_soma}")
print(f"Subtraindo: {x} - {y} = {resultado_subtracao}")
